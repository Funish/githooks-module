{
  "name": "@funish/hooks-module",
  "version": "0.0.0",
  "description": "Programmatically create hooks, powered by Funish.",
  "private": true,
  "scripts": {
    "build": "pnpm -r prepack",
    "lint:js": "eslint --ext \".js,.mjs,.ts\" --ignore-path .gitignore",
    "lint:prettier": "prettier --check . --ignore-path .gitignore",
    "lint": "pnpm lint:js && pnpm lint:prettier",
    "lintfix": "prettier --write --list-different . --ignore-path .gitignore && pnpm lint:js --fix",
    "postinstall": "hooks install"
  },
  "lint-staged": {
    "*.{js,mjs,ts}": "eslint --cache",
    "*.**": "prettier --check --ignore-unknown"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Funish/hooks-module.git"
  },
  "keywords": [
    "hooks"
  ],
  "author": {
    "name": "Funish",
    "email": "official@funish.net",
    "url": "https://funish.net/"
  },
  "contributors": [
    {
      "name": "Demo Macro",
      "email": "abc@imst.xyz",
      "url": "https://imst.xyz/"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Funish/hooks-module/issues"
  },
  "homepage": "https://github.com/Funish/hooks-module#readme",
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@funish/eslint-config": "0.0.1-0",
    "@funish/githooks": "workspace:*",
    "@types/node": "^17.0.23",
    "@types/parse-git-config": "^3.0.1",
    "eslint": "^8.13.0",
    "lint-staged": "^12.3.8",
    "prettier": "^2.6.2",
    "typescript": "^4.6.3",
    "unbuild": "^0.7.2"
  }
}
